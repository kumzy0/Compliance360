@page
@model Compliance360.Pages.TicketingModel
@{
     ViewData["Title"] = "Ticketing";
    Layout = "_DashboardLayout";
}

<div class="row">
    <div class="col-lg-4">
        <a asp-page="NewTicketApplication">
             <div class="card">
          <div class="card-header d-flex pb-1">
              <div class="avatar bg-rgba-primary p-50 m-0">
                  <div class="avatar-content">
                      <i class="feather icon-users text-primary font-medium-5"></i>
                  </div>
              </div>
              <div>
                   <h2 class="text-bold-700 mb-25">92.6k</h2>
              <p class="mb-0">New ticket application</p>
              </div>
             
          </div>
          
        </div>
        </a>
       
    </div>

    <div class="col-lg-4">
        <a asp-page="InProgressTicketApplication">
            <div class="card">
          <div class="card-header d-flex justify-content-between  align-items-start pb-1">
              <div class="avatar bg-rgba-primary p-50 m-0">
                  <div class="avatar-content">
                      <i class="feather icon-users text-primary font-medium-5"></i>
                  </div>
              </div>
              <div>
                  <h2 class="text-bold-700 mb-25">92.6k</h2>
              <p class="mb-0">Application in progress</p>
              </div>
              
          </div>
          
        </div>
        </a>
        
    </div>

    <div class="col-lg-4">
        <a asp-page="CompletedTicketApplication">
             <div class="card">
          <div class="card-header d-flex justify-content-between align-items-start pb-1">
              <div class="avatar bg-rgba-primary p-50 m-0">
